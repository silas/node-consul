{
  "compilerOptions": {
    "module": "node16",
    "lib": ["es6"],
    "moduleResolution": "Node16",
    "strict": true,
    "alwaysStrict": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": { "consul": ["../../lib"] }
  }
}
